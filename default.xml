<?xml version="1.0" encoding="UTF-8"?>

<manifest>

    <!-- use https mothed for repository. -->
    <remote name="AplexOS"
        fetch="https://github.com/AplexOS" />

    <!-- if you want to fetch just a branch from repository, set sync-c="true". -->
    <default 
        remote="AplexOS"
        sync-c="true"   
        sync-j="4" />

    <!-- local path name = server path name -->

    <!-- U-Boot Specification -->
    <project remote="AplexOS" revision="master" name="U-Boot" />
    <!-- Linux Kernel Specification -->
    <project remote="AplexOS" revision="master" name="Linux-Kernel" />
    <!-- Android Specification -->
    <project remote="AplexOS" revision="master" name="Android-Examples" />
    <!-- Linux Specification -->
    <project remote="AplexOS" revision="master" name="Linux-Examples" />
    <!-- Windows Specification -->
    <project remote="AplexOS" revision="master" name="Windows-Examples" />
    <!-- Qt Specification -->
    <project remote="AplexOS" revision="master" name="Qt-Examples" />
    <!-- Development Specification -->
    <project remote="AplexOS" revision="master" name="DevTools" />
    <!-- Simplified chinese Specification -->
    <project remote="AplexOS" revision="master" dest-branch="master" name="zh-cmn-Hans" />
    <!-- Manifest Specification -->
    <project remote="AplexOS" revision="master" name="Manifest" />

</manifest>
